{
  "schemaVersion": 1,
  "id": "arrowpopper",
  "version": "${version}",

  "name": "arrowpopper",
  "description": "a small mod that makes arrows and tridents shot by a player pop off as items instead of despawning",
  "authors": [
    "miir"
  ],
  "contact": {
    "homepage": "https://github.com/itsmiir/",
    "sources": "https://github.com/itsmiir/arrowpopper"
  },

  "license": "CC0-1.0",
  "icon": "assets/arrowpopper/icon-mosscairn.png",

  "environment": "*",
  "mixins": [
    "arrowpopper.mixins.json"
  ],
  "accessWidener" : "arrowpopper.accesswidener",
  "depends": {
    "fabricloader": "*",
    "minecraft": "*",
    "java": ">=17"
  }
}
